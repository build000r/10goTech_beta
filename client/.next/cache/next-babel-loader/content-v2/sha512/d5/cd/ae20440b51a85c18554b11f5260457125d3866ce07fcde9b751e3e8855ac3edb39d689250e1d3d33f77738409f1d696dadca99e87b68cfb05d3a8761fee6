{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useState, Fragment } from \"react\";\nimport { Form } from \"semantic-ui-react\";\n\nconst Checkbox = (clickOptions, handleSelected) => {\n  const {\n    0: checked,\n    1: setChecked\n  } = useState([]);\n\n  const handleToggle = c => () => {\n    const currentClickOpt = checked.indexOf(c); //returns first index of which given element is found in array OR returns -1 if not found\n\n    const newClickOpts = [...checked]; // if currenly checked was not already in checked state then -> push, else pull/take off\n\n    if (currentClickOpt === -1) {\n      newClickOpts.push(c);\n    } else {\n      newClickOpts.splice(currentClickOpt, 1);\n    }\n\n    setChecked(newClickOpts);\n    handleSelected(newClickOpts);\n  };\n\n  return clickOptions.map((c, i) => {\n    return /*#__PURE__*/_jsx(Fragment, {\n      children: /*#__PURE__*/_jsx(Form.Checkbox, {\n        onChange: handleToggle(c),\n        value: checked.indexOf(c === -1),\n        label: c.name\n      })\n    }, i);\n  });\n};\n\nexport default Checkbox;","map":null,"metadata":{},"sourceType":"module"}